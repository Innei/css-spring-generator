{
  "rewrites": [
    {
      "source": "/share/:path*",
      "destination": "https://follow-topaz.vercel.app/:path*"
    },
    {
      "source": "/og/:path*",
      "destination": "https://follow-topaz.vercel.app/og/:path*"
    },
    {
      "source": "/external-dist/:path*",
      "destination": "https://follow-topaz.vercel.app/:path*"
    }
  ]
}